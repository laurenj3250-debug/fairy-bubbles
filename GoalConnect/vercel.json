{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET", "HEAD"]
    }
  ]
}
